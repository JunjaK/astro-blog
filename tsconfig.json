{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "~/*": [
        "./src/*"
      ],
      "#/*": [
        "./*"
      ]
    },
    "types": [
      "unplugin-icons/types/astro",
      "unplugin-icons/types/svelte",
      "unplugin-icons/types/vue"
    ],
    "allowJs": true,
    "allowImportingTsExtensions": true,
    "noEmit": true
  },
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ]
}